   "source": [
    "minus_split = input().split('-')\n",
    "num = []\n",
    "\n",
    "for i in minus_split:\n",
    "    add = 0\n",
    "    s = i.split('+')\n",
    "    for j in s:\n",
    "        add += int(j)\n",
    "        \n",
    "    num.append(add)\n",
    "\n",
    "for i in range(len(num)):\n",
    "    if i == 0:\n",
    "        result = num[0]\n",
    "        continue\n",
    "    result -= num[i]\n",
    "print(result)"
   ]
  }
